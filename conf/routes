# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# 原来的案例以及相应的接口路由
GET     /                           controllers.ApplicationController.index
POST    /add                        controllers.ApplicationController.addUser
GET     /delete/:id                 controllers.ApplicationController.deleteUser(id : Long)


GET    /weixin                      controllers.WeixinController.checkToken(signature:String,timestamp:String,nonce:String,echostr:String)
POST   /weixin                      controllers.WeixinController2.xmlreponse
POST   /weixin2                     controllers.WeixinController2.xmlreponse


GET     /xiaofan1                   controllers.TestController.xiaofan1
GET     /xiaofan2                   controllers.TestController.xiaofan2
GET     /xiaofan3                   controllers.TestController.xiaofan3
GET     /xiaofan4                   controllers.TestController.xiaofan4
GET     /xiaofan5                   controllers.TestController.xiaofan5
GET     /xiaofan6                   controllers.TestController.xiaofan6
GET     /json1                      controllers.TestController.json1
GET     /json2                      controllers.TestController.json2
GET     /json3                      controllers.TestController.json3

GET     /r1                         controllers.TestController.r1
GET     /r2                         controllers.TestController.r2
GET     /r3                         controllers.TestController.r3
GET     /r4                         controllers.TestController.r4
GET     /pinyin                     controllers.TestController.pinyin

POST    /previewR                   controllers.PreviewController.previewR

POST    /reportR                        controllers.ReportController.addReport
POST    /listOwnerReport                controllers.ReportController.listOwnerReport
GET     /getOwnerReport/:owner          controllers.ReportController.getOwnerReport(owner:String)
GET     /getOwnerminiReport/:owner      controllers.ReportController.getOwnerminiReport(owner:String)

GET     /restrict1                  @controllers.RestrictController.restrictOne
GET     /restrict2                  @controllers.RestrictController.restrictTwo
GET     /restrict3                  @controllers.RestrictController.restrictThree
GET     /restrict4                  @controllers.RestrictController.restrictFour

GET     /rhp_roleHolderPresent      @controllers.SubjectPresentController.loggedIn
GET     /rhp_roleHolderNotPresent   @controllers.SubjectPresentController.notLoggedIn
GET     /rhnp_roleHolderPresent     @controllers.SubjectNotPresentController.loggedIn
GET     /rhnp_roleHolderNotPresent  @controllers.SubjectNotPresentController.notLoggedIn

GET     /setsessions                controllers.TestController.setsessions
GET     /getsessions                controllers.TestController.getsessions
GET     /rmsessions                 controllers.TestController.rmsessions

GET     /innerJoin1                  controllers.TestController.innerJoin1
GET     /innerJoin2                  controllers.TestController.innerJoin2
GET     /innerJoin3                  controllers.TestController.innerJoin3

POST    /localidentityadd                        controllers.IdentityController.AddIdentity
POST    /localidentityIdentityLogin              controllers.IdentityController.LoginIdentity
GET     /localidentityIdentityLogout             controllers.IdentityController.LogoutIdentity



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

